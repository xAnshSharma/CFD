FoamFile
{
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    leftSide
    {
        type            fixedValue;
        value		uniform (1 0 0);

    }
    rightSide
    {
        type            zeroGradient;
    }
	
    movingWall
    {
        type            fixedValue;
        value		uniform (3 0 0);
    }
    fixedWall
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //
