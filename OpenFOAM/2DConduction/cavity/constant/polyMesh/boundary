5
(
    leftSide
    {
        type            patch;
        nFaces          50;
        startFace       4900;
    }
    rightSide
    {
        type            patch;
        nFaces          50;
        startFace       4950;
    }
    movingWall
    {
        type            wall;
        inGroups        1(wall);
        nFaces          50;
        startFace       5000;
    }
    fixedWall
    {
        type            wall;
        inGroups        1(wall);
        nFaces          50;
        startFace       5050;
    }
    frontAndBack
    {
        type            empty;
        inGroups        1(empty);
        nFaces          5000;
        startFace       5100;
    }
)

